<script type="text/javascript">
	$(function(){
		var	 _boxall='.index_jdt'			//图片外面的DIVclass
			 _IMG   ='.index_jdt a'		//图片class
			 _anniuL='.anniu1'		//左侧按钮
			 _anniuR='.anniu2'		//右侧按钮
			 _sudu  =500;			//设置旋转速度
			 
			/*只需修改上面4个参数,支持N张图片*/
			 _imgW1 =$(_IMG).first().find('img').width();	//获取图片宽度，需要在img里面定义图片宽度
			 _imgH1 =$(_IMG).first().find('img').height();	//获取图片高度，需要在img里面定义图片高度
			 _boxW  =_imgW1*2.040;		//调整参数可以改变img外面box的宽度.不调整则按默认比例.
			 _imgW2 =_imgW1*0.6;		//调整参数可以改变后面2个img的宽度.不调整则按默认比例.
			 _imgH2 =_imgH1*0.6;		//调整参数可以改变后面2个img的高度.不调整则按默认比例.
			 _imgL  =_imgW1*0.36;		//调整参数可以改变最前面img的left距离.不调整则按默认比例.
			 _imgL2 =_imgW1*2*0.556;		//调整参数可以改变最右边img的left距离.不调整则按默认比例.
		$(_boxall).css({'width':'980px','height':'572px','position':'relative'});
		$(_IMG).hide();
		$(_IMG).css({'position':'absolute' });
		$(_IMG).first().next().show().css({'left':_imgL,'z-index':'3'});
		$(_IMG).first().show().css({'left':'0','top':(_imgH1-_imgH2)/2,'z-index':'2','opacity':'0.8'}).find('img').animate({width:_imgW2,height:_imgH2},0);
		$(_IMG).eq(2).show().css({'left':_imgL2,'top':(_imgH1-_imgH2)/2,'z-index':'1','opacity':'0.8'}).find('img').animate({width:_imgW2,height:_imgH2},0);
		
			$(_anniuR).click(function(){
					var _IMG2=$(_boxall).find("a");
						last_img = _IMG2.last();
						_firstW  = $(_IMG).first().position().left;
					if(_firstW==0){
			_IMG2.first().animate({left:_imgL,top:'0',height:_imgH1,width:_imgW1},_sudu).find('img').animate({width:_imgW1,height:_imgH1},_sudu);
			_IMG2.first().next().animate({width:_imgW2,left:_imgL2,top:(_imgH1-_imgH2)/2,height:_imgH2},_sudu).find('img').animate({width:_imgW2,height:_imgH2},_sudu);
			_IMG2.last().css({'height':_imgH2,'width':_imgW2,'z-index':'1','top':(_imgH1-_imgH2)/2}).animate({left:'0',top:(_imgH1-_imgH2)/2,height:_imgH2,width:_imgW2},_sudu).find('img').animate({width:_imgW2,height:_imgH2},_sudu);
			
			_IMG2.first().before(last_img);
			
			setTimeout("$(_IMG).first().next().css({'z-index':'3','opacity':'1'})",_sudu/2.5);
			setTimeout("$(_IMG).first().css({'opacity':'0.8','z-index':'1'})",_sudu/2.5);
			setTimeout("$(_IMG).eq(2).css({'opacity':'0.8','z-index':'2'})",_sudu/2.5);
			setTimeout("$(_IMG).eq(3).hide();",_sudu/2.5);
			}
			});

				if($(_IMG).length>3){
					$(_anniuL).click(function(){
					var last_img = $(_IMG).first();
						_firstW2 = $(_IMG).first().position().left;
						if(_firstW2==0){
			$(_IMG).first().next().animate({width:_imgW2,left:'0',top:(_imgH1-_imgH2)/2,height:_imgH2},_sudu).find('img').animate({width:_imgW2,height:_imgH2},_sudu);
			$(_IMG).eq(2).animate({left:_imgL,top:'0',height:_imgH1,width:_imgW1},_sudu).find('img').animate({width:_imgW1,height:_imgH1},_sudu);
			$(_IMG).eq(3).css({'left':_imgL,'height':_imgH2,'width':_imgW2,'z-index':'1','top':(_imgH1-_imgH2)/2}).show().animate({width:_imgW2,left:_imgL2,top:(_imgH1-_imgH2)/2,height:_imgH2},_sudu/1.5).find('img').animate({width:_imgW2,height:_imgH2},_sudu/1.5);
			$(_IMG).last().after(last_img);
			setTimeout("$(_IMG).last().hide()",_sudu/2.5);
			setTimeout("$(_IMG).eq(1).css('z-index','3')",_sudu/2.5);
			setTimeout("$(_IMG).first().css({'opacity':'0.8','z-index':'1'})",_sudu/2);
			setTimeout("$(_IMG).eq(2).css('opacity','0.8')",_sudu/2.5);
			setTimeout("$(_IMG).first().next().css({'opacity':'1','z-index':'3'})",_sudu/2.5);
						}
			});
				}
				else{
					$(_anniuL).click(function(){
					var last_img = $(_IMG).first();
						_firstW2 = $(_IMG).first().position().left;
						if(_firstW2==0){
			$(_IMG).first().next().animate({width:_imgW2,left:'0',top:(_imgH1-_imgH2)/2,height:_imgH2},_sudu).find('img').animate({width:_imgW2,height:_imgH2},_sudu);
			$(_IMG).first().animate({left:_imgL2,top:(_imgH1-_imgH2)/2,height:_imgH2,width:_imgW2},_sudu).find('img').animate({width:_imgW2,height:_imgH2},_sudu);
			$(_IMG).last().animate({left:_imgL,top:'0',height:_imgH1,width:_imgW1},_sudu).find('img').animate({width:_imgW1,height:_imgH1},_sudu);
			$(_IMG).last().after(last_img);
			setTimeout("{$(_IMG).first().css('z-index','2'),$(_IMG).last().css('z-index','1'),$(_IMG).first().next().css('z-index','3'),$(_IMG).first().css('opacity','0.6'),$(_IMG).last().css('opacity','0.6'),$(_IMG).first().next().css('opacity','1')}",_sudu/2.5);
						}
			});
					}
					
		 });
</script>
