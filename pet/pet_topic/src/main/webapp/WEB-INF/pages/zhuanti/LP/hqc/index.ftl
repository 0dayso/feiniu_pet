  <script type="text/javascript">       
$(function(){
  var num=0;
  $(".img_num li").mouseover(function(){
    var index=$(this).index();
    num=index;
    $(this).addClass("now").siblings().removeClass("now");
    $(".bigImg li").eq(num).fadeIn(2000).siblings().hide();
  });
    
  function imgScroll(){
    $(".img_num li").eq(num).addClass("now").siblings().removeClass("now");
    $(".bigImg li").eq(num).fadeIn(500).siblings().hide();
    num=num+1;
    if(num>$(".bigImg li").length-1) num=0;
  }
    var timer=setInterval(imgScroll,2000);
    $("#Slide_pic").hover(function(){
        clearInterval(timer);
      },
      function(){
        timer = setInterval(imgScroll,2000);      
      }
    );
  });

</script>

