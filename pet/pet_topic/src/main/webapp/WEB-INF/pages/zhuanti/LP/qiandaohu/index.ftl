 				Img.src = $("#big_pic >img").eq(index)[0].src;
				img_width=Img.width;
				marginLeft=img_width/2;
				$("#big_pic").css({"width":img_width,"margin-left":-marginLeft,"top":top+50});
				$("#big_pic,#ban_bg").show();
				$("#big_pic >img").hide().eq(index).show();
			});
		});
</script>
