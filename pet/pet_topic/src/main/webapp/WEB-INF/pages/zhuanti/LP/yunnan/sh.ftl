<script type="text/javascript">
		function city_SetTag(number, Tag) {
			var TableHtml = "<div id='city_data" + number + "'>" + $("#" + Tag + " .city_conts #city_data" + number).html() + "</div>";
			$("#" + Tag + " .city_conts #city_data" + number).remove();
			$("#" + Tag + " .city_conts").prepend(TableHtml);
		}
		$(function(){
			var index=0;
			var top;
			var _height=$(document).height();
			if(!window.XMLHttpRequest){
				$("#ban_bg").css({"position":"absolute","top":0,"height":_height});
				}
			//点击关闭
			$("#big_pic .close").click(function(){
					$(this).parent().hide();
					$("#ban_bg").hide();
			});
			//点击显示大图片
			$(".map_cont .map_big").click(function(){
				top=$(document).scrollTop();
				$("#big_pic").css("top",top+100);
				$("#big_pic,#ban_bg").show();
			});
			// 旅游城市切换
			var $c_nav = $("#city_nav li");
			//var $city_con = $("#city_cont .city_data");
			$c_nav.hover(function(){
				//var c_index = $c_nav.index(this);
				$(this).addClass("cur").siblings().removeClass("cur");
				//$city_con.eq(c_index).show().siblings().hide();
			});
			// 跟随菜单
			var $f_menu = $("#fix_nav");
			$(window).scroll(function(){ 
				var _scroll=$(document).scrollTop(); 
				if(_scroll>0){ 
					$f_menu.show(); 
				}else{ 
					$f_menu.hide(); 
				} 
			}); 
			
			$f_menu.find(".close").click(function(){
				$(this).parent().hide();
			});
		});
</script>
