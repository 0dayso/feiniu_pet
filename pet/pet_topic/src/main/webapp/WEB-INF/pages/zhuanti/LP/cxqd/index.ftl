		$('.xiazhou_but').removeClass('huise').addClass('zise');
		$('.xiazhou').animate({'top':200},300,function(){$('.shangzhou').animate({'top':110},300)});
		}
		});
	
	
	$('.cp_list_js3').find('li:gt(3)').hide();
	$('.cp_list_js3').each(function(){			  
		if($(this).find('li').length<5){$(this).next('.zhankai').hide();}
										});
	
	$('.zhankai').toggle(function(){
	$(this).prev('.cp_list_js3').find('li:gt(3)').slideDown();
	$(this).text('点击收起');
								  },function(){
	$(this).prev('.cp_list_js3').find('li:gt(3)').slideUp();
	$(this).text('点击展开');
								  });
	
								  
	var _num2 = 0;
	$('.jdtBnt li').hover(function(){ 
		 
		var _num1 = $(this).index();
		_num2 = _num1;
		$(this).addClass('jdtBnt_li').siblings().removeClass('jdtBnt_li');
		$('.jdtList li').eq(_num1).show().siblings().hide();
	});
	
	function zidong(){
		$('.jdtBnt li').eq(_num2).addClass('jdtBnt_li').siblings().removeClass('jdtBnt_li');
		$('.jdtList li').eq(_num2).show().siblings().hide();
		_num2++;
		if(_num2>$('.jdtBnt li').length-1)_num2=0;
	};
	var timer = setInterval(zidong,3000)
	$('.jdtBnt li,.jdtList').hover(function(){clearInterval(timer);},function(){timer = setInterval(zidong,3000);})
	
	
	
	})
</script>

